	name = Craig Heier
	email = cheier@arhcercom.com
[core]
	editor = subl -n -w
	excludesfile = /Users/qui-gon/.gitignore_global
[alias]
	st = status
	co = checkout
	br = branch
	cm = commit
	unstage = reset --HEAD
	last = log -1 HEAD
  	hist = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative

